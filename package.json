    "compression-webpack-plugin": "7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "^29.4.1",
    "source-map-loader": "2.0.2",
    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "^29.0.5",
    "ts-loader": "9.4.2",
    "typescript": "^4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
